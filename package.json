{
  "name": "schedule-services",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "db:migrate:status": "ts-node node_modules/.bin/sequelize db:migrate:status",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "ts-node node_modules/.bin/sequelize db:migrate:undo",
    "db:drop": "ts-node node_modules/.bin/sequelize db:drop",
    "db:create": "ts-node node_modules/.bin/sequelize db:create",
    "db:migration:create": "ts-node node_modules/.bin/sequelize migration:create --name",
    "db:seed-dev": "ts-node node_modules/.bin/sequelize db:seed:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:migrate:down:file": "ts-node node_modules/.bin/sequelize db:migrate:undo --name ${1}",
    "start:dev": "NODE_ENV=development nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --forceExit",
    "test:silent": "jest --silent",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:sequential": "jest --runInBand --silent",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "test:db:migrate": "NODE_ENV=test npm run db:migrate",
    "test:db:undo": "NODE_ENV=test npm run db:migrate:undo",
    "test:db:drop": "NODE_ENV=test npm run db:drop",
    "test:db:create": "NODE_ENV=test npm run db:create",
    "test:db:reset": "npm run test:db:drop && npm run test:db:create && npm run test:db:migrate",
    "migration:generate": "npx sequelize-cli migration:generate --name ${1}",
    "seed:generate": "npx sequelize-cli seed:generate --name ${1}"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@monmedx/monmedx-common": "^4.2.3",
    "@nestjs/common": "^7.0.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.0.1",
    "@nestjs/event-emitter": "0.0.3",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/sequelize": "^0.2.0",
    "@nestjs/swagger": "^4.7.7",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/id-generator-aws-xray": "^0.25.0",
    "@opentelemetry/instrumentation": "^0.25.0",
    "@opentelemetry/instrumentation-express": "^0.25.0",
    "@opentelemetry/instrumentation-http": "^0.25.0",
    "@opentelemetry/instrumentation-mysql2": "^0.25.0",
    "@opentelemetry/propagator-aws-xray": "^0.25.0",
    "@opentelemetry/sdk-trace-node": "^0.25.0",
    "aws-sdk": "^2.948.0",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.6",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.0.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.25",
    "pubsub-service": "npm:@monmedx/monmedx-pubsub@^1.0.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "sequelize": "6.6.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.24",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.14.6",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-silent-reporter": "^0.5.0",
    "lint-staged": "^10.5.3",
    "nock": "^13.1.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "graphql/*.graphql": [
      "graphql-schema-linter graphql/schemas/appointment-all.graphql"
    ]
  }
}
